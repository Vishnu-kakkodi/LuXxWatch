    <script>
        const form = document.getElementById('otpform');
        form.addEventListener('submit', async function submitForm(event) {
            event.preventDefault();

            const preformData = new FormData(form);
            const formData = new URLSearchParams(preformData);

            try {
                const response = await fetch('/verify', {
                    method: 'POST',
                    body: formData
                });

                const data = await response.json();

                if (response.ok) {
                    // Item remove successfully

                    swal({
                        title: "Wow!",
                        text: "Successfully verified Otp",
                        type: "success"
                    }).then(function () {
                        window.location.href = "/login";
                    });
                    console.error('Verified otp');


                } else {
                    // Handle error
                    const errorMessage = document.getElementById('otpmessage');
                    errorMessage.innerText = data.message;
                }
                removeInnerText();
                function removeInnerText() {
                    const message = document.getElementById('otpmessage');
                    setTimeout(() => {
                        message.innerText = '';
                    }, 3000);
                }
            } catch (error) {
                console.error('Error:', error);
            }

        });
    </script>

    <script>
        async function resendOtp() {
            clearInterval(timerInterval);
            countdownTime = 60;
            const timerSpan = document.getElementById('timer');
            timerSpan.innerText = countdownTime;
            const resendForm = document.getElementById('resend-otp-form');
            resendForm.style.display = 'none';
