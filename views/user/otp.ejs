                                        <div id="otp-timer" style="font-size: 18px; font-weight: bold; color: #ff0000; text-align: center;">
                                            Time left: <span id="timer">60</span> seconds
                                        </div>

                                        <form id="resend-otp-form" style="display: none; text-align: center;">
                                            <button type="button" class="btn btn-fill-out btn-block hover-up mt-3"
                                                onclick="resendOtp()">Resend OTP</button>
                                        </form>


                                        <script>
                                            let countdownTime = 60; // Initial countdown time
                                            let timerInterval; // Variable to hold the timer interval

                                            function updateTimer() {
                                                const timerSpan = document.getElementById('timer');

                                                if (countdownTime > 0) {
                                                    countdownTime--;
                                                    timerSpan.innerText = countdownTime;
                                                } else {
                                                    clearInterval(timerInterval);
                                                    const resendForm = document.getElementById('resend-otp-form');
                                                    resendForm.style.display = 'block'; // Show the resend OTP form
                                                }
                                            }

                                            function startTimer() {
                                                timerInterval = setInterval(updateTimer, 1000); // Start the timer
                                            }

                                            startTimer(); // Start the timer when the page loads

                                            function resendOtp() {
                                                clearInterval(timerInterval); // Clear the existing timer interval
                                                // Your code to resend OTP goes here
                                                countdownTime = 60; // Reset countdown time
                                                const timerSpan = document.getElementById('timer');
                                                timerSpan.innerText = countdownTime; // Update timer display
                                                const resendForm = document.getElementById('resend-otp-form');
                                                resendForm.style.display = 'none'; // Hide the resend OTP form again
                                                startTimer(); // Start the timer again
                                            }
                                        </script>

                                    </div>
                                </div>
                            </div>
