    <style>
        .custom-btn {
            border-radius: 0 5px 5px 0;
            width: 80px;
            border-style: none;
            background-color: white;
        }

        .nav-container {
            display: flex;
        }

        .flex-container {
            display: flex;
            justify-content: space-between;
            list-style: none;
            /* Remove default list styles */
            padding: 0;
            /* Remove default padding */
            margin-bottom: 10px;
        }

        .flex-container li {
            margin-right: 20px;
            /* Adjust spacing between list items */
        }

        .flex-container li:last-child {
            margin-right: 0;
            /* Remove margin from last list item */
        }

        .flex-container li a {
            text-decoration: none;
            /* Remove underline from links */
            color: black;
            /* Set default color */
            transition: color 0.3s;
            /* Add transition for smooth color change */
            font-size: medium;
            font-weight: bold;
            margin-right: 80px;
        }

        .dropdown {
            position: relative;
            display: inline-block;
        }

        .dropdown-toggle {
            text-decoration: none;
            color: black;
            font-size: medium;
            font-weight: bold;
            padding: 8px 12px;
        }

        .dropdown-menu {
            display: none;
            position: absolute;
            background-color: #fff;
            border: 1px solid #ccc;
            border-radius: 5px;
            padding: 8px 0;
            z-index: 1;
        }

        .dropdown-menu option {
            display: block;
            padding: 8px 12px;
            font-size: medium;
            font-weight: bold;
            text-decoration: none;
            color: black;
        }

        .dropdown-menu option:hover {
            background-color: #a994f8;
        }

        .dropdown:hover .dropdown-menu {
            display: block;
        }



        .flex-container li a:hover {
            color: rgb(247, 189, 0);
            /* Change color on hover */
        }

        .svgInject {
            filter: invert(100%) sepia(0%) saturate(0%);
            /* Change color to white */
            color: white;
        }
    </style>

    <script>
        async function searchData(event) {
            event.preventDefault();
            const searchItem = document.getElementById('searchItem').value;
            try {
                // Construct the URL based on the search input value
                const url = `/searchItem?searchItem=${encodeURIComponent(searchItem)}`;
                console.log(url);
                // Redirect the user to the constructed URL
                window.location.href = url;
            } catch (error) {
                console.error('Error:', error);
            }
        }
    </script>






